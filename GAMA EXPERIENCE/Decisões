INICIO 

    DECLARE NOTA1, NOTA2, MEDIA

    LEIA(NOTA1)
    LEIA(NOTA2)

    MEDIA = (NOTA1 + NOTA2) / 2

    ESCREVA("A MEDIA FINAL VALE")
    ESCREVA(MEDIA)

    SE MEDIA > 9 ENTAO
        ESCREVA ("CONCEITO A")
    SENAO
        SE MEDIA > 7 ENTAO
            ESCREVA("CONCEITO B")
        SENAO
            SE MEDIA > 5 ENTAO
                ESCREVA("CONCEITO C")
            SENAO
            ESCREVA("REPROVADO(A)")
        FIMSE
    FIMSE